<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.phoenix"
    xmlns:android="http://schemas.android.com/apk/res/android" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="activities.DemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activities.alertbox.AlertBoxActivity"
            android:parentActivityName="activities.DemoActivity"/>
        <activity android:name="activities.callus.CallUsActivity"
            android:parentActivityName="activities.DemoActivity"/>
        <activity android:name="activities.dialog.CustomAlertDialogActivity"
            android:parentActivityName="activities.DemoActivity"/>
        <activity android:name="activities.divider.DividerActivity"
            android:parentActivityName="activities.DemoActivity"/>
        <activity android:name="activities.dialoginoutbox.DialogInputBoxActivity" />
        <activity android:name="activities.emptystate.EmptyStateActivity" />
        <activity android:name="activities.overlay.OverlayActivity" />
    </application>
</manifest>
